{
  "manifest_version": 3,
  "version": "1.2",
  "name": "youtube live chat viewer",
  "description": "some descâ„¢",
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "react-app-holder.js"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "notifications"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "icons/*",
        "inject.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}